<div class="row expirations">
  <div class="col-md-3">
    <table class="table table-striped table-bordered table-small table-hover">
      <thead>
        <tr>
          <th>Expiration</th>
          <th>Total Quantity</th>
        </tr>
      </thead>
      <tbody>
        {{#each this}}
        <tr>
          <td>{{$expiration this.key.[3]}}</td>
          <td>{{$num this.value.sum}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
