<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Item</th>
      <th>Category</th>
      <th>Date</th>
      <th>From</th>
      <th>To</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    {{#each this}}
      <tr>
        <td>{{this.key.[0]}}</td>
        <td>{{this.key.[1]}}</td>
        <td>{{$dateFormat this.key.[2]}}</td>
        <td>{{this.key.[3]}}</td>
        <td>{{this.key.[4]}}</td>
        <td>{{$currency this.value}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
