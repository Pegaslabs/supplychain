<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>From</th>
      <th>To</th>
      <th>Transactions</th>
      <th>Total Value</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    {{#each this}}
      <tr>
        <td>{{$dateFormat this.key.[1]}}</td>
        <td>{{this.key.[2]}}</td>
        <td>{{this.key.[3]}}</td>
        <td>{{this.key.[4]}}</td>
        <td>{{$currency this.value}}</td>
        <td><a href="{{$baseUrl}}shipment/{{this.id}}">shipment</a></td>
      </tr>
    {{/each}}
  </tbody>
</table>
