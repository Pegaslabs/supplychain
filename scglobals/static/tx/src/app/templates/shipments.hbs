<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>From</th>
      <th>To</th>
      <th>Transactions</th>
      <th>Value</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    {{#each this}}
      <tr>
        <td>{{$dateFormat this.date}}</td>
        <td>{{this.from_location_name}}</td>
        <td>{{this.to_location_name}}</td>
        <td>{{this.total_transactions}}</td>
        <td>{{$currency this.total_value}}</td>
        <td><a href="#/shipment/{{this._id}}">details</a></td>
      </tr>
    {{/each}}
  </tbody>
</table> 
