<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>From</th>
      <th>To</th>
      <th>Transactions</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    {{#each shipments}}
      <tr>
        <td>{{$dateFormat this.transactions.[0].shipment_date}}</td>
        <td>{{this.transactions.[0].from_location_name}}</td>
        <td>{{this.transactions.[0].to_location_name}}</td>
        <td>{{this.transaction_count}}</td>
        <td>{{$currency this.total_value}}</td>
      </tr>
    {{/each}}
  </tbody>
</table> 
